{
  "name": "ai-product-recommendation-system",
  "version": "1.0.0",
  "description": "AI-Powered Product Recommendation System built with Cursor",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:backend": "cd backend && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "build": "cd frontend && pnpm run build",
    "test": "cd backend && pnpm test && cd ../frontend && pnpm test",
    "start": "cd backend && pnpm start"
  },
  "keywords": ["ai", "recommendation", "system", "cursor", "fullstack"],
  "author": "Cursor AI Development",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 